{
  "name": "ad-distribution",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "domains/ad-distribution/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/domains/ad-distribution",
        "configFile": "domains/ad-distribution/vite.config.mts",
        "tsConfig": "domains/ad-distribution/tsconfig.app.json",
        "type": "module"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "ad-distribution:build",
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "ad-distribution:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "ad-distribution:build:production",
          "hmr": false
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "ad-distribution:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
