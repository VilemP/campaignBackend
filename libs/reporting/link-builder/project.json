{
  "name": "reporting-link-builder",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/reporting/link-builder/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/reporting/link-builder",
        "main": "libs/reporting/link-builder/src/index.ts",
        "tsConfig": "libs/reporting/link-builder/tsconfig.lib.json",
        "assets": [
          "libs/reporting/link-builder/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "config": "libs/reporting/link-builder/vitest.config.ts",
        "reportsDirectory": "coverage/libs/reporting/link-builder"
      }
    }
  },
  "tags": [
    "scope:reporting",
    "type:feature"
  ]
}
